
@{
    ViewBag.Title = "Information";
    Layout = "~/Views/Shared/_LayoutPagehead.cshtml";
}
<div style="text-align:center;margin-top:50px"><h1>个人信息</h1></div>
<div class="container" style="margin-top:50px;">

    <table class="table tbale-bordered table-hover">
        <tr>
            <td>学号</td>
            <td>@Model.s_id</td>
        </tr>
        <tr>
            <td>姓名</td>
            <td>@Model.s_name</td>
        </tr>
        <tr>
            <td>年龄</td>
            <td>@Model.s_age</td>
        </tr>
        <tr>
            <td>手机号码</td>
            <td>@Model.s_phone</td>
        </tr>
        <tr>
            <td>性别</td>
            <td>@Model.s_sex</td>
        </tr>
        <tr>
            <td>家庭住址</td>
            <td>@Model.s_address</td>
        </tr>
    </table>
</div>
